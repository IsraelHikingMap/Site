{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "israel-hiking": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": {
              "base": "wwwroot",
              "browser": ""
            },
            "index": "src/index.html",
            "tsConfig": "./tsconfig.app.json",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "assets": [
              "src/content",
              "src/translations",
              "src/resource-not-found.html",
              "src/empty-for-oauth.html",
              "src/googlef59316cdb0845a8c.html",
              "src/robots.txt",
              "src/fonts/glyphs",
              "src/fonts/glyphs.json",
              {
                "glob": "**/mapbox-gl-rtl-text.js",
                "input": "./node_modules/@mapbox/mapbox-gl-rtl-text/dist",
                "output": "./"
              }
            ],
            "styles": [
              "src/scss/common.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/scss"
              ]
            },
            "scripts": [],
            "allowedCommonJsDependencies": [
              "geojson-rbush",
              "redux-undo",
              "photoswipe",
              "photoswipe-ui-default",
              "invert-color",
              "lottie-web",
              "detect-node",
              "rbush",
              "maplibre-gl",
              "@mapbox/vector-tile",
              "pbf",
              "linear-interpolator",
              "piexifjs",
              "deepmerge",
              "cordova-plugin-purchase",
              "xml-beautify",
              "d3-regression",
              "platform",
              "osm2geojson-lite",
              "piexif-ts"
            ],
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "browser": "src/main.ts",
            "server": "src/main.server.ts",
            "outputMode": "static"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "mobile": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "outputPath": {
                "base": "www",
                "browser": ""
              },
              "baseHref": "./",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.mobile.ts"
                }
              ]
            },
            "mobile-debug": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb"
                }
              ],
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "outputPath": {
                "base": "www",
                "browser": ""
              },
              "baseHref": "./",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.mobile-debug.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "options": {
            "buildTarget": "israel-hiking:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "israel-hiking:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "israel-hiking:build"
          }
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "codeCoverageExclude": [
              "src/application/services/database.service.ts",
              "src/application/services/geo-location.service.ts",
              "src/application/services/connection.service.ts",
              "src/application/components/**",
              "src/application/directives/**",
              "src/application/pipes/**",
              "src/application/reducers/**"
            ],
            "karmaConfig": "./karma.conf.cjs",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "./tsconfig.spec.json",
            "scripts": [],
            "assets": [
              "src/content",
              "src/translations",
              "src/resource-not-found.html",
              "src/empty-for-oauth.html"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss",
      "type": "component"
    },
    "@schematics/angular:directive": {
      "prefix": "app",
      "type": "directive"
    },
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  },
  "cli": {
    "analytics": "e3974bec-e594-4e85-be50-efbf3d593d0a"
  }
}