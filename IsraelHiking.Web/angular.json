{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "israel-hiking": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "wwwroot",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "./tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/content",
              "src/translations",
              "src/resource-not-found.html",
              "src/empty-for-oauth.html",
              "src/googlef59316cdb0845a8c.html",
              "src/robots.txt",
              "src/fonts/glyphs",
              "src/fonts/glyphs.json",
              {
                "glob": "**/mapbox-gl-rtl-text.js",
                "input": "./node_modules/@mapbox/mapbox-gl-rtl-text",
                "output": "./"
              }
            ],
            "styles": [
              "src/scss/common.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/scss"
              ]
            },
            "scripts": [],
            "allowedCommonJsDependencies": [
              "geojson-rbush",
              "redux-undo",
              "photoswipe",
              "photoswipe-ui-default",
              "invert-color",
              "lottie-web",
              "detect-node"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "mobile": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "outputPath": "www",
              "baseHref": "./",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.mobile.ts"
                }
              ]
            },
            "mobile-debug": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb"
                }
              ],
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "outputPath": "www",
              "baseHref": "./",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.mobile-debug.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "israel-hiking:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "israel-hiking:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "israel-hiking:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverageExclude": [
              "src/application/services/database.service.ts",
              "src/application/components/**",
              "src/application/directives/**",
              "src/application/pipes/**",
              "src/application/reducers/**"
            ],
            "karmaConfig": "./karma.conf.js",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "./tsconfig.spec.json",
            "scripts": [],
            "assets": [
              "src/content",
              "src/translations",
              "src/resource-not-found.html",
              "src/empty-for-oauth.html"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    },
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  },
  "cli": {
    "analytics": "e3974bec-e594-4e85-be50-efbf3d593d0a"
  }
}
