{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "israel-hiking": {
      "root": "",
      "sourceRoot": "sources",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "wwwroot",
            "index": "sources/index.html",
            "main": "sources/main.ts",
            "tsConfig": "./tsconfig.app.json",
            "polyfills": "sources/polyfills.ts",
            "assets": [
              "sources/content",
              "sources/translations",
              "sources/resource-not-found.html",
              "sources/empty-for-oauth.html",
              "sources/googlef59316cdb0845a8c.html",
              "sources/robots.txt",
              "sources/cordova.js",
              "sources/fonts/glyphs",
              "sources/fonts/glyphs.json",
              {
                "glob": "**/mapbox-gl-rtl-text.js",
                "input": "./node_modules/@mapbox/mapbox-gl-rtl-text",
                "output": "./"
              }
            ],
            "styles": [
              "sources/scss/common.scss"
            ],
            "scripts": [
              "sources/google-analytics.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "sources/environments/environment.ts",
                  "with": "sources/environments/environment.prod.ts"
                }
              ]
            },
            "cordova": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "outputPath": "www",
              "baseHref": "./",
              "fileReplacements": [
                {
                  "replace": "sources/environments/environment.ts",
                  "with": "sources/environments/environment.cordova.ts"
                }
              ]
            },
            "cordova-debug": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": false,
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "outputPath": "www",
              "baseHref": "./",
              "fileReplacements": [
                {
                  "replace": "sources/environments/environment.ts",
                  "with": "sources/environments/environment.cordova-debug.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "israel-hiking:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "israel-hiking:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "israel-hiking:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "sources/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "sources/polyfills.ts",
            "tsConfig": "./tsconfig.spec.json",
            "scripts": [
              "sources/google-analytics.js"
            ],
            "assets": [
              "sources/content",
              "sources/translations",
              "sources/resource-not-found.html",
              "sources/empty-for-oauth.html"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "israel-hiking-e2e": {
      "root": "",
      "sourceRoot": "e2e",
      "projectType": "application"
    }
  },
  "defaultProject": "israel-hiking",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
