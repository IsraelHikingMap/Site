{
	"appId": "com.mapeak",
	"appName": "Mapeak",
	"webDir": "www",
	"server": {
		"errorPath": "/"
	},
	"cordova": {
		"preferences": {
			"OAuthScheme": "mapeak"
		}
	},
	"plugins": {
		"SystemBars": {
			"insetsHandling": "disable"
		}
	},
	"ios": {
		"preferredContentMode": "mobile"
	},
	"packageClassList": [
		"KeepAwakePlugin",
		"ScreenBrightnessPlugin",
		"AppPlugin",
		"CAPCameraPlugin",
		"DevicePlugin",
		"SharePlugin",
		"TextZoomPlugin",
		"AppUpdatePlugin",
		"BackgroundGeolocation",
		"CapgoCompassPlugin",
		"PurchasesPlugin",
		"EmailComposerPlugin",
		"CDVPlugin"
	]
}
